{
  "expo": {
    "name": "Scroll",
    "slug": "scroll",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/scroll-icon.png",
    "splash": {
      "image": "./assets/images/scroll-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#52aefc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
